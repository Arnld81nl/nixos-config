# Secrets configuration - COPY to secrets.nix and fill in your values
# This file is gitignored - your actual secrets.nix will not be committed
{
  # Git user email
  gitEmail = "your-email@example.com";

  # 1Password configuration
  onePassword = {
    # Your 1Password account identifier
    account = "my";

    # Vault/item paths for app-backup
    ageKey = "op://Private/age-key/private-key";
    sshKey = "op://Private/ssh-key/private key";
  };

  # VPN Configuration (also stored in ~/.config/vpn/config at runtime)
  vpn = {
    rsg = {
      host = "0.0.0.0:10443";
      opItem = "Vault/VPN-Item";
      cert = "";
    };
    dnv = {
      host = "0.0.0.0:443";
      opItem = "Vault/VPN-Item";
      cert = "";
    };
    esdal = {
      host = "0.0.0.0:443";
      opItem = "Vault/VPN-Item";
      cert = "";
    };
  };
}
